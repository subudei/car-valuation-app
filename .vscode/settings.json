{
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "files.associations": {
      "*.css": "tailwindcss"
    },
    "cSpell.language": "en,nb",
    "cSpell.words": [
      "favourite",
      "favourites",
      "hookform",
      "trpc"
    ],
  }